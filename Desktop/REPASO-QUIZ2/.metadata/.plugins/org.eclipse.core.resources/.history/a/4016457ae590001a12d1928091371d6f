package main;

public abstract class Figura {

	private int posx;
	private int posy;
	private int velx;
	private int vely;
	private int tamano;
	private int colorR;
	private int colorG;
	private int colorB;

	public Figura(int px, int py, int colR, int colG, int colB) {

		posx = px;
		posy = py;
		colorR = colR;
		colorcG = colG;
		colorcB = colB;
		velx = 4;
		vely = 4;
		tamano = 50;
	}
	// metodo moverse

	public abstract void moverse();

	public abstract void pintar();

}
